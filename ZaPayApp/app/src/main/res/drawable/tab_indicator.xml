<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">
    <item android:gravity="center">
        <shape>
            <size
                android:width="25dp"
                android:height="4dp" />
            <corners android:radius="10dp" />
            <solid android:color="#FE5E00" />
        </shape>
    </item>
</layer-list>








