<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="advDrawerLayout">
        <attr name="adl_customBehavior" format="boolean" />
        <attr name="adl_viewScale" format="float" />
        <attr name="adl_viewScrimColor" format="color" />
        <attr name="adl_viewElevation" format="dimension" />
        <attr name="adl_drawerElevation" format="dimension" />
        <attr name="adl_cardBackgroundColor" format="reference|color" />
    </declare-styleable>

    <declare-styleable name="CustomRatingBar">
        <attr name="maxStars" format="integer"/>
        <attr name="stars" format="float"/>
        <attr name="starOff" format="reference|integer"/>
        <attr name="starOn" format="reference|integer"/>
        <attr name="starHalf" format="reference|integer"/>
        <attr name="starPadding" format="dimension"/>
        <attr name="onlyForDisplay" format="boolean"/>
        <attr name="halfStars" format="boolean"/>
    </declare-styleable>

</resources>